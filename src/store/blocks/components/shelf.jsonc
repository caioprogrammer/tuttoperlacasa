		"title": "Banner Prateleira desktop",
		"children": ["stack-layout#shelf-banner-cta.desktop"],
		"props": {
			"width": "44%"
		}
	},
	"flex-layout.col#shelf-banner-content": {
		"title": "Prateleira com banner",
		"children": ["hover-container#shelf-banner"],
		"props": {
			"blockClass": "shelf-banner-content",
			"width": "56%"
		}
	},
	"rich-text#shelf-home01": {
		"title": "Prateleira 01 título",
		"props": {
			"text": "### Os mais vendidos\nUma seleção de produtos mais vendidos para trazer requinte ao seu ambiente",
			"blockClass": ["shelf-home01", "common-title"]
		}
	},
	"rich-text#shelf-home02": {
		"title": "Prateleira 02 título",
		"props": {
			"text": "### Marcas dos sonhos\nProdutos feitos por marcas renomadas agora na Tutto",
			"blockClass": ["shelf-home02", "common-title"]
		}
	},
	"rich-text#shelf-suggestions": {
		"title": "Prateleira sugestões título",
		"props": {
			"text": "### Os queridinhos da nossa loja\nSe você se interessou pelo produto maravilhoso acima, vai se interessar por esses também:",
			"blockClass": ["shelf-suggestions", "common-title"]
		}
	},
	"stack-layout#shelf-banner-cta.desktop": {
		"title": "Banner prateleira desktop",
		"children": ["image#banner-shelf-desktop"],
		"props": {
			"blockClass": "shelf-banner-cta"
		}
	},
	"stack-layout#shelf-banner-cta.mobile": {
		"title": "Banner prateleira mobile",
		"children": ["image#banner-shelf-mobile"],
		"props": {
			"blockClass": "shelf-banner-cta"
		}
	},
	"link#shelf-banner-button": {
		"title": "Botão banner prateleira",
		"children": ["rich-text#shelf-banner-button-text", "icon#arrow-cta-shelf"],
		"props": {
			"href": "",
			"label": "",
			"blockClass": "shelf-banner-button"
		}
	},
	"rich-text#shelf-banner-button-text": {
		"props": {
			"text": "__CONHEÇA TODA A COLEÇÃO__",
			"blockClass": "shelf-banner-button-text"
		}
	},
	"icon#arrow-cta-shelf": {
		"props": {
			"id": "nav-arrow-right",
			"size": 14,
			"activeClassName": "vtex-store-components-3-x-arrowCTAIcon"
		}
	},
	"hover-container#shelf-home01": {
		"children": ["list-context.product-list#shelf-home01"],
		"props": {
			"blockClass": ["shelf-home01", "hover-shelf-list"]
		}
	},
	"hover-container#shelf-home02": {
		"children": ["list-context.product-list#shelf-home02"],
		"props": {
			"blockClass": ["shelf-home02", "hover-shelf-list"]
		}
	},
	"hover-container#shelf-banner": {
